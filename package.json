{
  "name": "vot-cli",
  "version": "1.2.0",
  "description": "A small script that allows you to download an audio translation from Yandex via the terminal.",
  "type": "module",
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "format": "prettier --write --ignore-unknown \"src/**/*.{js,ts,json}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FOSWLY/vot-cli.git"
  },
  "keywords": [
    "cli",
    "vot",
    "vot-cli",
    "voice-over-translation",
    "downloader",
    "foswly"
  ],
  "author": "Toil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FOSWLY/vot-cli/issues",
    "email": "toil.contact@yandex.com"
  },
  "homepage": "https://github.com/FOSWLY/vot-cli/#readme",
  "dependencies": {
    "axios": "^1.5.0",
    "chalk": "^5.3.0",
    "listr2": "^6.6.1",
    "minimist": "^1.2.8",
    "protobufjs": "^7.2.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.3"
  }
}
