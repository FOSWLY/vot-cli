  "type": "module",
  "main": "./src/index.js",
  "bin": "./src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "npx eslint .",
    "lint-fix": "npx eslint . --fix",
    "format": "prettier --write --ignore-unknown \"src/**/*.{js,ts,json}\"",
    "prepare": "husky",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FOSWLY/vot-cli.git"
  },
